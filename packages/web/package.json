{
  "name": "web",
  "private": true,
  "scripts": {
    "dev:env": "vercel pull ../.. --env packages/web/.env.development.local",
    "dev:server": "next dev",
    "dev:start": "yarn dev:env && yarn dev:server",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write ./**/*.{json,js,ts,tsx}",
    "test:e2e": "cypress run",
    "test:unit": "ts-mocha -p tests/unit/tsconfig.json tests/unit/*.spec.ts",
    "algolia:index:localhost": "yarn build:addresses && node scripts/indexContract.js localhost http://127.0.0.1:8545/",
    "algolia:setup": "node scripts/setupAlgolia.js",
    "algolia:seed": "node scripts/seedTitles.js",
    "img:compress": "imageoptim public/images/*"
  },
  "dependencies": {
    "algoliasearch": "^4.13.0",
    "dotenv": "^16.0.0",
    "fortmatic": "^2.2.1",
    "iconoir": "^1.0.0",
    "iconoir-react": "^4.8.1",
    "joi": "^17.6.0",
    "next": "^12.1.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-instantsearch-dom": "^6.23.3",
    "react-toastify": "^8.2.0",
    "styled-components": "^5.3.3",
    "web3": "^1.7.3",
    "web3modal": "^1.9.5"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "17.0.12",
    "@types/react": "17.0.38",
    "@types/react-instantsearch-dom": "^6.12.2",
    "@types/styled-components": "^5.1.24",
    "chai": "^4.3.6",
    "cypress": "^9.3.1",
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.9",
    "mocha": "^9.2.0",
    "prettier": "2.5.1",
    "ts-mocha": "^9.0.2",
    "ts-node": "^10.5.0",
    "typescript": "4.5.5"
  }
}
